{
  "name": "blockchainiz",
  "version": "1.3.2",
  "description": "A node library to interact more easily with the Blockchainiz API",
  "main": "index.js",
  "scripts": {
    "lint": "prettier --write \"source/**/*.js\" \"test/**/*.js\" && eslint --fix --ext .js --cache source/ test/",
    "pretest": "npm run lint",
    "test": "mocha --reporter spec --timeout 20000",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "security-check": "nsp check -o summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utocat/lib-blockchainiz-node.git"
  },
  "keywords": [
    "blockchain",
    "blockchainiz",
    "api",
    "bitcoin",
    "ethereum"
  ],
  "author": "Utocat (Kevin Bockelandt)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/utocat/lib-blockchainiz-node/issues"
  },
  "homepage": "https://github.com/utocat/lib-blockchainiz-node#readme",
  "dependencies": {
    "coveralls": "^2.11.12",
    "dotenv": "^2.0.0",
    "eslint-config-utocat": "git+https://deployutocat:hKD6jPCwpqGwNuUhY3LW@gitlab.com/utocat/eslint-config-utocat.git",
    "istanbul": "^0.4.4",
    "mocha": "^4.0.1",
    "prettier": "^1.10.2",
    "request": "^2.74.0",
    "should": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "nsp": "^3.1.0"
  }
}
